Negocios <br>

<%= form_tag search_negocios_path do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", name: nil %>
  </p>

<% end %>

<ul>
<% for negocio in @negocios %>
<li>
  <%= link_to negocio.nombre, negocio_path(negocio) %>
  <%= link_to "Edit", edit_negocio_path(negocio) %>
  <%= link_to "Destroy", negocio_path(negocio), method: :delete %>
</li>
<% end %>
</ul>

<p> <%= link_to "Agregar Negocio", new_negocio_path %> </p>